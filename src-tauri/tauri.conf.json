{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "lol-afk",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "path": {
        "all": true
      },
      "fs": {
        "scope": ["$DATA/*"]
      }
    },
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "lol-afk",
      "icon": [
        "icons/icon32x32.png",
        "icons/icon128x128.png",
        "icons/icon128x128@2x.png",
        "icons/icon.ico"
      ],
      "resources": [
        "./data/*"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "LoL AFK",
        "width": 861,
        "height": 600,
        "minWidth": 545,
        "minHeight": 380,
        "url": "./html/index.html"
      }
    ]
  }
}
